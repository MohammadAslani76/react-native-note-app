{
  "expo": {
    "name": "یادداشت ها",
    "slug": "note-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/note-image.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/note-image.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/note-image.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mohammad_aslani.noteapp"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "c501f77e-73b4-4cd6-826e-6525093b69ab"
      }
    }
  }
}
